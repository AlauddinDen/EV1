@model IEnumerable<EV3.Models.CourseHeadCount>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Default</title>
    <link href="~/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body>
    <table class="table table-bordered">
        <tr>
            <th>Course</th>
            <th>Count</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CourseN</td>
                <td>@item.Count</td>
            </tr>
        }
    </table>
</body>
</html>
